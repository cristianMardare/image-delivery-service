{
  "name": "image-delivery-service",
  "version": "0.0.1",
  "description": "An image delivery service for Ownzones Node.js Challenge",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm run clean && npm run build-ts && node dist/index.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev:watch": "nodemon --watch src -e ts --exec npm run start",
    "clean": "rimraf ./dist"
  },
  "author": "Cristian Mardare",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "4.17.1",
    "@types/jest": "25.1.3",
    "express": "4.17.1",
    "jest": "25.1.0",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "module-alias": "2.2.2",
    "nodemon": "2.0.2",
    "rimraf": "3.0.2",
    "sharp": "0.24.1"
  },
  "_moduleAliases": {
    "@api/image": "dist/api/components/image",
    "@services": "dist/services"
  }
}
